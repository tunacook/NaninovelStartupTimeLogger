<linker>
    <!-- asmdef の name と一致させる -->
    <assembly fullname="NaninovelStartupTimeLogger">
        <type fullname="NaninovelStartupTimeLogger.StartupTimeLogger" preserve="all"/>
    </assembly>

    <!-- 反射で触る可能性のある Naninovel 側 -->
    <assembly fullname="Elringus.Naninovel.Runtime">
        <type fullname="Naninovel.IScriptPlayer" preserve="all"/>
        <type fullname="Naninovel.PlayedScript"  preserve="all"/>
        <type fullname="Naninovel.Script"        preserve="all"/>
    </assembly>
</linker>
